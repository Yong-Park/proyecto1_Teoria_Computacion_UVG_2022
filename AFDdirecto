digraph {
	fake [style=invisible]
	B [shape=doublecircle]
	E [shape=doublecircle]
	A [root=true shape=doublecircle]
	D [shape=doublecircle]
	C
	F [shape=doublecircle]
	fake -> A [style=bold]
	B -> C [label=a]
	E -> D [label=b]
	F -> A [label=b]
	D -> C [label=a]
	A -> B [label=a]
	E -> C [label=a]
	F -> C [label=a]
	B -> E [label=b]
	C -> D [label=b]
	A -> F [label=b]
	rankdir=LR
}
